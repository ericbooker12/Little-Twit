<ul>
	<% @tweets.each do |tweet|%>
		<% date = tweet.created_at - (8 * 60 * 60) %>
		<li><span class="user">
			<strong><%=  tweet.user.username.capitalize %></strong>
		</span>
		<span class="timestamp">
			<%= date.strftime("at %I:%M%p on %m/%d/%Y") %>
		</span>
		<span class="content">
			<%= tweet.content %>
		</span>
		<hr>
		</li>
	<% end %>
</ul>